{
  "name": "ssupago", 
  "version": "1.0",
  "description": "Improve your will.",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icon-96.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["floating_button.js"]
    }
  ],
  "web_accessible_resources": ["popup.html"],
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "https://*/*",
    "http://*/*"
  ]
}