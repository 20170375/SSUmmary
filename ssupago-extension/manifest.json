{
  "name": "SSuPago", 
  "version": "1.0",
  "description": "모든 외국페이지 번역&요약",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icon-96.png",
    "default_popup": "html/setting.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["css/floating_button.css"],
      "js": ["js/floating_button.js"]
    }
  ],
  "web_accessible_resources": ["icon-96.png", "html/popup.html"],
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "https://*/*",
    "http://*/*"
  ]
}